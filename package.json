{
  "name": "Boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "kysely:codegen": "kysely-codegen --out-file=src/infrastructure/kysely/types.ts"
  },
  "imports": {
    "#domain/*": "./src/domain/*.js",
    "#models/*": "./app/models/*.js",
    "#infrastructure/*": "./src/infrastructure/*.js",
    "#controllers/*": "./src/infrastructure/controllers/*.js",
    "#middleware/*": "./src/infrastructure/middleware/*.js",
    "#config/*": "./config/*.js",
    "#providers/*": "./providers/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.10.16",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.13.2",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.20.1",
    "hot-hook": "^0.4.0",
    "kysely-codegen": "^0.18.5",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.0",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.7.3",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.3.1",
    "@adonisjs/core": "^6.17.2",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^3.1.0",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/shield": "^8.1.2",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@inertiajs/react": "^2.0.9",
    "@vinejs/vine": "^3.0.0",
    "edge.js": "^6.2.1",
    "kysely": "^0.28.2",
    "luxon": "^3.6.1",
    "pg": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.2"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "volta": {
    "node": "24.0.1",
    "npm": "11.3.0"
  }
}
